---
layout: layout-base
bootstrap_container: <%= stache.config.bootstrap_container %>
---

<div class="content">
  <div class="{{ bootstrap_container }}">
    <div class="row">
      <div class="col-sm-12">
        {{# inherit stache.config.markdown markdown }}
          {{# markdown }}{{ body }}{{/ markdown }}
        {{ else }}
          {{# withNewline }}{{ body }}{{/ withNewline }}
        {{/ inherit }}
      </div>
    </div>
  </div>
</div>
